{% trans_default_domain 'FOSUserBundle' %}


<form action="{{ path('fos_user_registration_register') }}" {{ form_enctype(form) }} method="POST" class="fos_user_registration_register">
	<p>
		<span class="label label-important">{{ 'form.generic.allFieldsRequired' | trans({}, 'IMDCTerpTubeBundle') }}</span>
	</p>
    {{ form_widget(form) }}
    <p>
        <input class="btn btn-success span1" type="submit" value="{{ 'registration.submit'|trans }}" />
    </p>
</form>
